{
  "name": "code-challenge-newsfeed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "codegen:graphql-server": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only graphql_server/schema",
    "codegen:graphql-client": "graphql-codegen --config graphql_client/codegen.yml",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@types/marked": "^1.2.2",
    "apollo-server-micro": "^2.21.0",
